rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /{document=**} {
      allow read, write: if false;
    }

    match /products/{productID}{
      allow read;
    }

    match /baskets/{userID}{
      allow get, write: if request.auth.uid == userID;
    }
  }
}